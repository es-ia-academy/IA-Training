</div></div><script id="yiOrKrk" data-type="js">document.addEventListener('DOMContentLoaded', (event) => {
    const heroOverlay = document.getElementById('hero-overlay');

    // Establecer estilo inicial para el overlay
    heroOverlay.style.opacity = 1;
    heroOverlay.style.position = 'absolute';
    heroOverlay.style.width = '100%';
    heroOverlay.style.height = '100%';
    heroOverlay.style.backgroundColor = 'rgba(0,0,0,0.5)';
    heroOverlay.style.transition = 'opacity 2s'; // Duración del efecto fade

    // Función para aplicar fade out
    function fadeOut() {
        heroOverlay.style.opacity = 0; // Hace el overlay completamente transparente
        setTimeout(fadeIn, 2000); // Espera 2 segundos antes de iniciar fadeIn
    }

    // Función para aplicar fade in
    function fadeIn() {
        heroOverlay.style.opacity = 1; // Vuelve a hacer el overlay visible
    }

    // Ejecutar fade out inicialmente para demostrar
    fadeOut();
});
</script><button class="toolbar-open" onclick="toolsOpenModal(this)"><img src="../global/file/edit-section.svg"></button></section>
    
